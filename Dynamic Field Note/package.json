{
  "name": "dynamicfieldnote",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "storybook": "storybook dev -p 6006",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "stylelint": "stylelint \"**/*.css\" --allow-empty-input",
    "stylelint:fix": "stylelint \"**/*.css\" --fix --allow-empty-input",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run stylelint && npm run format:check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest e2e/smoke --ci --maxWorkers=2",
    "test:local": "jest e2e --coverage --verbose",
    "test:smoke": "jest e2e/smoke",
    "test:comprehensive": "jest e2e/comprehensive --coverage",
    "test:e2e": "npm run test:smoke && npm run test:comprehensive",
    "test:e2e:ui": "playwright test",
    "test:e2e:ui:headed": "playwright test --headed",
    "test:e2e:ui:debug": "playwright test --debug",
    "test:e2e:ui:report": "playwright show-report",
    "test:all": "npm run test:smoke && npm run test:comprehensive && npm run test -- src/ && npm run test:e2e:ui",
    "quality": "npm run format:check && npm run lint && npm run stylelint && npm run type-check",
    "quality:fix": "npm run format && npm run lint:fix && npm run stylelint:fix && npm run type-check",
    "perf:flashlight": "flashlight test --config flashlight.config.js",
    "perf:flashlight:report": "flashlight report --open",
    "perf:android": "flashlight measure android",
    "perf:lighthouse": "lhci autorun",
    "perf:lighthouse:mobile": "lighthouse http://localhost:8080 --preset=mobile --view",
    "perf:lighthouse:desktop": "lighthouse http://localhost:8080 --preset=desktop --view",
    "perf:web": "npm run web && npm run perf:lighthouse",
    "perf:all": "npm run perf:flashlight && npm run perf:lighthouse",
    "perf:update-badges": "node scripts/generate-performance-badge.js",
    "perf:update-history": "node scripts/update-performance-history.js",
    "perf:report": "npm run perf:lighthouse && npm run perf:update-badges && npm run perf:update-history",
    "optimize:images": "node scripts/optimize-images.js",
    "generate:blurhash": "node scripts/generate-blurhash.js",
    "prepare:images": "npm run optimize:images && npm run generate:blurhash",
    "precommit": "npm run quality && npm run test:smoke",
    "prepare": "echo 'Skipping husky install'",
    "guardrails": "npm run quality && npm run test:smoke",
    "guardrails:full": "npm run quality && npm run test:e2e",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@react-navigation/drawer": "^7.5.10",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/native-stack": "^7.3.28",
    "blurhash": "^2.0.5",
    "expo": "~54.0.13",
    "expo-camera": "^17.0.8",
    "expo-file-system": "^19.0.17",
    "expo-image": "^3.0.9",
    "expo-image-manipulator": "^14.0.7",
    "expo-secure-store": "^15.0.7",
    "expo-splash-screen": "^31.0.10",
    "expo-sqlite": "^16.0.8",
    "expo-status-bar": "~3.0.8",
    "jszip": "^3.10.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "^4.1.3",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.12.1",
    "react-native-web": "^0.21.0",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.38.0",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.56.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@storybook/addon-a11y": "^9.1.13",
    "@storybook/addon-docs": "^9.1.13",
    "@storybook/addon-onboarding": "^9.1.13",
    "@storybook/addon-vitest": "^9.1.13",
    "@storybook/react-native-web-vite": "^9.1.13",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "better-sqlite3": "^12.4.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-storybook": "^9.1.13",
    "jest": "~29.7.0",
    "lighthouse": "^12.8.2",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "react-test-renderer": "^19.1.0",
    "sharp": "^0.34.4",
    "storybook": "^9.1.13",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "ts-jest": "^29.4.5",
    "typescript": "~5.9.2",
    "vite": "^7.1.10",
    "vitest": "^3.2.4",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "private": true
}
